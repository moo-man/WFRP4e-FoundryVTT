<section class="tab {{tab.cssClass}}" data-group="primary" data-tab="{{tab.id}}">
    {{formGroup fields.ammunitionType.fields.value value=source.system.ammunitionType.value}}
    <fieldset>
        <legend>{{localize "Weapon Modifiers"}}</legend>
        {{formGroup fields.range.fields.value value=source.system.range.value}}
        {{formGroup fields.damage.fields.value value=source.system.damage.value}}
        {{formGroup fields.damage.fields.dice value=source.system.damage.dice}}
    </fieldset>
    {{> itemProperties}}
    {{formGroup fields.special.fields.value value=source.system.special.value}}
</section>